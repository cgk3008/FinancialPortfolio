@model FinancialPortal.Models.AdminHousehold
@{
	ViewBag.Title = "AddToHouse";
}

<html>
<body>


	<div class="right_col" role="main">
		<div class="">

			@*<div class="clearfix"></div>*@
			<div class="row">
				<div class="col-md-12 col-sm-12 col-xs-12">
					<div class="x_panel">
						<div class="x_title">
							<h1>Add Person To Household <small></small></h1>
							<div class="x_content">

							</div>
							@*<div class="clearfix"></div>*@
						</div>
						<div class="x_content">
							<p class="text-muted font-13 m-b-30">

							</p>

							<p>



								@using (Html.BeginForm())
								{
									@Html.AntiForgeryToken()

									@Html.HiddenFor(m => m.Household.Id)

									@Html.ListBox("SelectedUsers", Model.Users, new { @class = "control-label col-md-2" })
									<br />
									<br />
									<br />
									<h5>
										Hold Ctrl Key to Select Multiple Users for the Project. <br /><br />
										Hold Shift Key and Scroll through List to Select Multiple Users or All Users.
									</h5>
									<br />
								<p><input type="submit" class="btn btn-info" /></p>

							}

							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		



		@section Scripts {
			@Scripts.Render("~/bundles/jqueryval")
		}
	</body>
</html>